<!DOCTYPE html>
<html lang="">

<head>
	<%-include(head) %>
		<link rel="stylesheet" href="css/appManager.css">
</head>

<body>
	<%-include(header) %>
		<header>
			<template id="FbPad">
				<div class="user-account">
					<div class="fb-avatar">
						<span class="username-text" app-role="fbName"></span>
						<span class="avatar-img"><img app-role = "fbAvatar">
                        </span>
					</div>
				</div>
			</template>
			<!-- User -->
			<div class="login-block">
				<div app-role='removeAfterLogin'>
					<button class="btn btn-facebook" id="bntFbLogin">
                        <i class="fa fa-facebook"></i><span>Login with Facebook</span>
                    </button>
				</div>
			</div>
			<!-- /User -->
			<!-- Search box -->
			<div id="searchBox" class="search-box">
				<div class="col-xs-12 col-sm-9 col-md-9">
					<span class="search-mode lnr" onclick="toggleSearch()"></span>
					<form role="search" class="search-inner">
						<i class="lnr lnr-magnifier"></i>
						<input type="text" name="q" placeholder="Search" class="form-control" style="border-color: #8f29b3;">
					</form>
				</div>
			</div>
			<!-- /Search box -->
		</header>
		<h1 class="text-center">
			<!-- <%=title %> -->
		</h1>
		<%-include(scripts) %>
			<div id="phasebeam">
				<canvas width="700" height="650"></canvas>
				<canvas width="700" height="650"></canvas>
				<canvas width="700" height="650"></canvas>
			</div>
			<script type="text/javascript" src="js/phasebeam.js"></script>
			<%-include(footer) %>

</body>

</html>