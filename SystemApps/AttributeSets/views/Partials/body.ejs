<!--main body of create AttributeSets in SystemApps/AttributeSets/views/Partials/body  -->
<div class="container-fluid attribute-sets" id="start-offset" style="min-height: 100%">
    <creatAttributePad>
        <template component-role="containerControl">
            <div class="row control" component-role="container_div" data-toggle="popover" data-placement="right">
                <div class="col-md-3 col-lg-3"><label data-controltype="label"></label></div>
                <div class="col-md-9 col-lg-9" component-role="inpuCover">
                    <span class="help-block col-md-12 col-lg-12" data-controltype="describe">Describe attribute</span></div>
            </div>
        </template>
        <template id='extraOptionImgHandler'>
            <div class="ctr-select-box" app-role="selectHandler" onfocus="imageOption.activeShow(this, event)" onblur="imageOption.hideShow(this)"
                tabindex="0">
                <div app-role="selectbox" class="ctr-select-value" onclick="imageOption.toggleShow(this, event)"> Select here
                </div>
                <div app-role="dropdown" class="ctr-select-down">
                    <div app-role="droppad">
                    </div>
                    <div class="s-bottom">
                        <a class="btn" onclick="imageOption.addmoreFn(this,event)" templHandler="imageHandler" comp-dimensions=[450,450]><i class="fa fa-plus"></i> Add More</a>
                    </div>
                </div>
            </div>
        </template>
        <template id='extraOptionImgItem'>
            <div class="ctr-select-option" app-role="selectItem" onclick='imageOption.getOptionImage(this)' onfocus="imageOption.itemFocuses(this)"
                tabindex="0">
                <!-- <div class="ctr-select-option" app-role="selectItem" tabindex="0">  -->
                <div class="s-img"><img app-role="attImg" src="" class="img-flag"></div>
                <div class="s-content">
                    <label app-role="attName"></label>
                    <div class="s-button">
                        <a class="btn" app-role="deleteImgOption" onclick="imageOption.deleteFn(this)"><i class="fa fa-times" ></i> Delete</a>
                        <a class="btn" app-role="editImgOption" app-action="selectImage" templHandler="imageHandler" comp-dimensions=[450,450]><i class="fa fa-pencil"></i> Edit</a>
                    </div>
                </div>
            </div>
            <script type="text/javascript">
                // registering dependent modules                
                selectImageHandler.initiate(imageOption.updateImageOption);
            </script>
        </template>
        
        <div class="container-fluid">
            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                <fieldset class="scheduler-border">
                    <legend class="scheduler-border">Create Attribute Sets </legend>
                    <div class="row">
                        <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2" id="SetsIcon">
                            <img src='imgs/logo.png' id="iconHolder" app-action = "simpleImg" templHandler = "imageHandler" onload = "setAttribIcon(this)" comp-dimensions=[300,300] style="border-radius: 50%;">
                        </div>
                        <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10" id="SetsName">
                            <legend data-controltype="label">Set's Name</legend>
                        </div>
                    </div>
                    <div component-role="receivedPad" class="col-xs-12 col-sm-12 col-md-12 col-lg-12"></div>
                </fieldset>
            </div>

            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                <fieldset class="scheduler-border">
                    <legend class="scheduler-border">Input Types</legend>
                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs" role="tablist">
                        <li class="active">
                            <a href="#standardElement" role="tab" data-toggle="tab">
                                <icon class="fa fa-home"></icon> Standard Elements
                            </a>
                        </li>
                        <li>
                            <a href="#timeElements" role="tab" data-toggle="tab" onclick="saveElement()">
                                <i class="fa fa-user"></i> Time Elements
                            </a>
                        </li>
                        <li>
                            <a href="#otherElements" role="tab" data-toggle="tab" onclick="saveElement()">
                                <i class="fa fa-user"></i> Other Elements
                            </a>
                        </li>
                        <li>
                            <a href="#jsonoutput" role="tab" data-toggle="tab" onclick="saveElement()">
                                <i class="fa fa-user"></i> JSON output
                            </a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div id="standardElement" class="connectedSortable tab-pane fade active in">
                            <!-- Controls Attribute start here -->
                        </div>
                        <div id="timeElements" class="connectedSortable tab-pane fade">
                            <!-- Controls Attribute start here -->
                        </div>
                        <div id="otherElements" class="connectedSortable tab-pane fade">
                            <!-- Controls Attribute start here -->
                        </div>
                        <div id="jsonoutput" class=" tab-pane fade " style="margin-top:25px">
                            <!-- Controls Attribute start here -->
                            <pre id="printJSON" class="container-fluid">
                                JSON Output
                    </pre>
                        </div>
                    </div>
                </fieldset>
            </div>
        </div>
    </creatAttributePad>
</div>
<!--End of main body of create AttributeSets in SystemApps/AttributeSets/views/Partials/body  -->